   "source": [
    "import cv2\n",
    "import os\n",
    "\n",
    "save_path = 'unprocessed_images'\n",
    "\n",
    "for path in os.listdir(f'{save_path}'):\n",
    "    if path.endswith(f'.PNG'):\n",
    "        image = cv2.imread(f'{save_path}/{path}')\n",
    "        image_name = path.split('.PNG')[0]\n",
    "        cv2.imwrite(f'{save_path}/{image_name}.JPG', image)"
   ]
  }
